<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.blend.androiddesignpattern">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/design"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".y_mvvm.demo2.MvvmMainActivity"></activity>
        <activity android:name=".o_visitor.butterknife.DemoActivity" />
        <activity android:name=".l_memento.Demo2.NodeMainActivity" />
        <activity android:name=".q_proxy.Binder.BankActivity" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/LaunchTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".x_mvp.optimize.ActiclesActivity" />
        <activity android:name=".o_visitor.aa_annotation.AnnotationActivity" />

        <service
            android:name=".q_proxy.Binder.BankService"
            android:process="com.blend.androiddesignpattern.q_proxy.Binder.BankService" />
    </application>

</manifest>